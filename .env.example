# Database
DATABASE_PATH=./data/coverage.db

# GitHub (required for private repos and pushing PRs)
GITHUB_TOKEN=your_github_token_here

# AI Providers (at least one required for test generation)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Job Processor (set to false to disable background job processing)
ENABLE_JOB_PROCESSOR=true

# Coverage Settings
COVERAGE_THRESHOLD=80

# AI Settings
AI_MAX_RETRIES=3
AI_TIMEOUT_MS=300000
